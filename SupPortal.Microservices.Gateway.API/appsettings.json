    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "secret-key": "secret-key-auth-jwt-token-supportal",
  "HealthChecksUI": {
